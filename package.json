{
  "name": "meshcentral-deploy",
  "version": "0.1.0",
  "private": true,
  "description": "Local + CI/CD deployment wrapper for MeshCentral",
  "scripts": {
    "start": "node node_modules/meshcentral/meshcentral.js",
    "start:dev": "NODE_ENV=development node node_modules/meshcentral/meshcentral.js",
    "postinstall": "node -e \"try{require('fs').mkdirSync('meshcentral-data',{recursive:true})}catch(e){}\""
  },
  "dependencies": {
    "meshcentral": "^1.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}

